{
  "name": "code-extractor",
  "version": "0.0.1",
  "description": "Code Extractor",
  "main": "build/src/index.js",
  "author": "immament",
  "license": "MIT",
  "files": [
    "build/lib"
  ],
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
