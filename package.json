    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "loglevel": "^1.7.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.10",
    "pinst": "^2.1.4",
    "ts-jest": "^26.5.0"
  },
  "engines": {
    "node": ">=10.6.0"
  }
}
